    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.4",
    "@prisma/client": "^5.19.1",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.9.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "nestjs-i18n": "^10.4.9",
    "pinia": "^2.2.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "spotify-api.js": "^9.2.5"
  },
