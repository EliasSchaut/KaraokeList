    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "prisma-error-enum": "^0.1.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2"
  },
  "overrides": {
    "vue": "latest"
  }
}
