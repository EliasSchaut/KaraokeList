{
  "name": "karaoke-list",
  "version": "1.0.0-alpha.2",
  "description": "A list of available songs with the possibility to report issues",
  "author": "EliasSchaut",
  "private": true,
  "license": "MIT",
  "homepage": "https://karaoke.schaut.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/EliasSchaut/KaraokeList"
  },
  "scripts": {
    "start:client": "node .output/server/index.mjs",
    "start:server": "nest start",
    "dev:client": "nuxt dev",
    "dev:server": "nest start --debug --watch",
    "build": "npm run build:server && npm run build:client && npm run build:db",
    "build:server": "nest build",
    "build:client": "nuxt build",
    "build:db": "npx prisma migrate dev --schema=./schema.prisma --name dev",
    "format": "prettier --write \\\"server/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.5",
